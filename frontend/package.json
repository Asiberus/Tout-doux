{
    "name": "frontend",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --open",
        "build": "vue-cli-service build --modern",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "lint": "eslint --ext .ts,.vue --fix",
        "format": "prettier . --write",
        "prepare": "cd .. && husky install frontend/.husky",
        "docker:build": "docker compose --env-file ../.conf/development/conf.env build",
        "docker:up": "docker compose --env-file ../.conf/development/conf.env up",
        "docker:prod:build": "docker compose --file ../docker-compose.prod.yml --env-file ../.conf/production/conf.local.env build",
        "docker:prod:up": "docker compose --file ../docker-compose.prod.yml --env-file ../.conf/production/conf.local.env up"
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "core-js": "^3.6.5",
        "moment": "^2.29.1",
        "register-service-worker": "^1.7.1",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^9.1.2",
        "vue-resource": "^1.5.2",
        "vue-router": "^3.2.0",
        "vuetify": "^2.6.4",
        "vuex": "^3.4.0",
        "vuex-module-decorators": "^1.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^14.1.0",
        "@commitlint/config-conventional": "13.2.0",
        "@types/jest": "^24.0.19",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-e2e-nightwatch": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-pwa": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.0.3",
        "chromedriver": "^84.0.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "husky": "^7.0.4",
        "lint-staged": "^11.2.6",
        "sass": "1.47.0",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.1",
        "sass-loader": "^8.0.2",
        "typescript": "4.9.5",
        "vue-cli-plugin-vuetify": "~2.2.2",
        "vue-template-compiler": "^2.6.11"
    },
    "volta": {
        "node": "14.18.1",
        "npm": "8.1.0"
    }
}
