{
  "name": "frontend",
  "version": "0.4.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ignore-pattern .gitignore --fix",
    "format": "prettier . --write",
    "prepare": "cd .. && husky install frontend/.husky",
    "docker:build": "docker compose --env-file ../.conf/development/conf.env build",
    "docker:up": "docker compose --env-file ../.conf/development/conf.env up",
    "docker:prod:build": "docker compose --file ../docker-compose.prod.yml --env-file ../.conf/production/conf.env build",
    "docker:prod:up": "docker compose --file ../docker-compose.prod.yml --env-file ../.conf/production/conf.env up"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "deep-equal": "^2.2.3",
    "moment": "^2.30.1",
    "pinia": "^2.2.6",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/js": "^9.14.0",
    "@types/deep-equal": "^1.0.4",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "eslint-plugin-vuetify": "^2.4.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "sass": "~1.80",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^6.0.1",
    "vue-eslint-parser": "^9.4.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-android-arm-eabi": "*",
    "@rollup/rollup-android-arm64": "*",
    "@rollup/rollup-darwin-arm64": "*",
    "@rollup/rollup-darwin-x64": "*",
    "@rollup/rollup-linux-arm-gnueabihf": "*",
    "@rollup/rollup-linux-arm-musleabihf": "*",
    "@rollup/rollup-linux-arm64-gnu": "*",
    "@rollup/rollup-linux-arm64-musl": "*",
    "@rollup/rollup-linux-powerpc64le-gnu": "*",
    "@rollup/rollup-linux-riscv64-gnu": "*",
    "@rollup/rollup-linux-s390x-gnu": "*",
    "@rollup/rollup-linux-x64-gnu": "*",
    "@rollup/rollup-linux-x64-musl": "*"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "volta": {
    "node": "18.19.1",
    "yarn": "1.22.19"
  }
}
